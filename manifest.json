{
  "manifest_version": 3,
  "name": "Quick Utah",
  "version": "2024.9",

  "action": {
    "default_popup": "popup/index.html"
  },
  "description": "Chrome extension that provides quick access to University of Utah Asia Campus-related websites",
  "icons": {
    "16": "images/icon-16.png",
    "32": "images/icon-32.png",
    "48": "images/icon-48.png",
    "128": "images/icon-128.png"
  },

  "author": "김예환 Ye-Hwan Kim (Sam)",
  "homepage_url": "https://github.com/yehwankim23/quick-utah",
  "permissions": ["tabs"]
}
